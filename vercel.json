{
  "version": 2,
  "buildCommand": "playwright install chromium",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0"
  },
  "functions": {
    "api/app.py": {
      "maxDuration": 300,
      "memory": 2048
    }
  },
  "regions": ["iad1"]
}
